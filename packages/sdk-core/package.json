{
  "name": "@open-auth/sdk-core",
  "version": "0.1.4",
  "description": "",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.32.34",
    "@fastify/type-provider-typebox": "^4.0.0",
    "axios": "1.7.2"
  },
  "devDependencies": {
    "tsup": "8.1.0",
    "typescript": "^5.5.2"
  },
  "tsup": {
    "entry": [
      "index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true
  },
  "files": [
    "dist/"
  ]
}
